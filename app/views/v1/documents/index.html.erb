<h1>Listing documents</h1>

<table>
  <tr>
    <th>Created by user</th>
    <th>Owned by user</th>
    <th>Title</th>
    <th>Keywords</th>
    <th>Questions</th>
    <th>Description</th>
    <th>Body</th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%=h document.created_by_user_id %></td>
    <td><%=h document.owned_by_user_id %></td>
    <td><%=h document.title %></td>
    <td><%=h document.keywords %></td>
    <td><%=h document.questions %></td>
    <td><%=h document.description %></td>
    <td><%=h document.body %></td>
    <td><%= link_to 'Show', document %></td>
    <td><%= link_to 'Edit', edit_document_path(document) %></td>
    <td><%= link_to 'Destroy', document, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New document', new_document_path %>